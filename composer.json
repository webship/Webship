{
  "name": "webship/webship",
  "description": "Helps in speeding up the work of web developers to ship websites in a swift way.",
  "type": "drupal-profile",
  "license": "GPL-2.0-or-later",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "authors": [
    {
      "name": "Webship",
      "homepage": "https://www.drupal.org/webship",
      "role": "Maintainer"
    }
  ],
  "support": {
    "issues": "http://drupal.org/project/issues/webship",
    "source": "http://cgit.drupalcode.org/webship"
  },
  "repositories": {
    "drupal": {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
      },
    "assets": {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  },
  "require": {
    "composer/installers": "^1.2",
    "cweagans/composer-patches": "^1.6.5",
    "drupal-composer/drupal-scaffold": "^2.5",
    "webflo/drupal-finder": "^1.0.0",
    "webmozart/path-util": "^2.3",
    "drupal/core": "^8.6.0",
    "drupal/config_provider": "^2.0",
    "drupal/config_snapshot": "^1.0",
    "drupal/config_merge": "^1.0",
    "drupal/config_sync": "^2.0",
    "drupal/config_distro": "^1.0",
    "drupal/config_update": "^1.0",
    "drupal/config_filter": "^1.0",
    "drupal/config_normalizer": "^1.0",
    "drupal/config_split": "^1.0",
    "drupal/config_ignore": "^2.0",
    "drupal/adminimal_theme": "^1.0",
    "drupal/admin_toolbar": "^1.0",
    "drupal/adminimal_admin_toolbar": "^1.0",
    "drupal/libraries": "^3.0",
    "drupal/ctools": "^3.0",
    "drupal/entity": "^1.0",
    "drupal/token": "^1.0",
    "drupal/ds": "^3.0",
    "drupal/field_group": "^3.0",
    "drupal/smart_trim": "^1.0",
    "drupal/pathauto": "^1.0",
    "drupal/uikit": "^3.0",
    "drupal/webform": "^5.0",
    "drupal/webtheme": "^1.0",
    "drupal/webpage": "^1.0",
    "drupal/webapps": "^1.0"
  },
  "require-dev": {
    "drupal/console": "^1.0.2",
    "drush/drush": "^9.0.0",
    "drupal/coder": "^8.0",
    "drupal/drupal-extension": "^3.0",
    "phing/phing": "^2.0",
    "behat/behat": "^3.0",
    "behat/mink": "^1.0",
    "behat/mink-extension": "^2.0",
    "behat/mink-goutte-driver": "^1.0",
    "behat/mink-zombie-driver": "^1.0",
    "behat/mink-selenium2-driver": "^1.0",
    "behatch/contexts": "^3.0",
    "emuse/behat-html-formatter": "^0.1"
  },
  "config": {
    "bin-dir": "bin/",
    "secure-http": false,
    "preferred-install": {
      "drupal/core": "dist"
    }
  },
  "autoload": {
    "psr-4": {
      "Webship\\composer\\": "src/composer"
    }
  },
  "scripts": {
    "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
    "pre-install-cmd": ["Webship\\composer\\Procedures::checkComposerVersionProcedure"],
    "pre-update-cmd": ["Webship\\composer\\Procedures::checkComposerVersionProcedure"],
    "post-install-cmd": [
      "Webship\\composer\\Procedures::createRequiredFilesProcedure",
      "@composer drupal-scaffold",
      "./bin/phing push"
    ],
    "post-update-cmd": [
      "Webship\\composer\\Procedures::createRequiredFilesProcedure",
      "./bin/phing push"
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-8.x-1.x": "8.1.x-dev"
    },
    "installer-types": [
      "bower-asset",
      "npm-asset"
    ],
    "installer-paths": {
      "web/core": ["type:drupal-core"],
      "web/profiles/{$name}": ["type:drupal-profile"],
      "web/modules/contrib/{$name}": ["type:drupal-module"],
      "web/themes/contrib/{$name}": ["type:drupal-theme"],
      "web/libraries/{$name}": [
          "type:drupal-library",
          "type:bower-asset",
          "type:npm-asset"
      ],
      "web/modules/custom/{$name}": ["type:drupal-custom-module"],
      "web/themes/custom/{$name}": ["type:drupal-custom-theme"],
      "drush/contrib/{$name}": ["type:drupal-drush"]
    },
    "enable-patching": true,
    "composer-exit-on-patch-failure": true,
    "patchLevel": {
      "drupal/core": "-p2"
    } 
  }
}
