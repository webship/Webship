{
  "name": "webship/webship",
  "description": "Helps in speeding up the work of web developers to ship websites in a swift way.",
  "type": "drupal-profile",
  "license": "GPL-2.0-or-later",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "authors": [
    {
      "name": "Webship",
      "homepage": "https://www.drupal.org/webship",
      "role": "Maintainer"
    }
  ],
  "support": {
    "issues": "http://drupal.org/project/issues/webship",
    "source": "http://cgit.drupalcode.org/webship"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    }
  ],
  "require": {
    "composer/installers": "1.5.0",
    "cweagans/composer-patches": "1.6.4",
    "drupal-composer/drupal-scaffold": "2.4.0",
    "drupal/drupal-library-installer-plugin": "0.3",
    "drupal/core": "8.5.0",
    "drupal/uikit": "3.0",
    "drupal/uikit_admin": "3.0-rc1",
    "drupal/uikit_components": "3.x-dev",
    "drupal/uikit_gui": "1.x-dev",
    "drupal/ctools": "3.0",
    "drupal/entity": "1.0-beta3",
    "drupal/config_update": "1.5",
    "drupal/token": "1.1",
    "drupal/page_manager": "4.0-beta2",
    "drupal/panels": "4.2",
    "drupal/ds": "3.1",
    "drupal/field_group": "3.0-beta1",
    "drupal/smart_trim": "1.1",
    "drupal/pathauto": "1.1",
    "drupal/features": "3.7",
    "drupal/webtheme": "1.0-alpha1",
    "drupal/webpage": "1.x-dev"
  },
  "require-dev": {
    "drush/drush": "8.1.16",
    "drupal/coder": "8.2.12",
    "drupal/drupal-extension": "3.4.1",
    "phing/phing": "2.16.1",
    "behat/behat": "3.4.3",
    "behat/mink": "1.7.1",
    "behat/mink-extension": "2.3.1",
    "behat/mink-goutte-driver": "1.2.1",
    "behat/mink-zombie-driver": "1.4.0",
    "behat/mink-selenium2-driver": "1.3.1",
    "behatch/contexts": "3.1.0",
    "emuse/behat-html-formatter": "0.1.0"
  },
  "config": {
    "bin-dir": "bin/",
    "secure-http": false
  },
  "autoload": {
    "psr-4": {
      "Webship\\composer\\": "src/composer"
    }
  },
  "scripts": {
    "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
    "pre-install-cmd": ["Webship\\composer\\Procedures::checkComposerVersionProcedure"],
    "pre-update-cmd": ["Webship\\composer\\Procedures::checkComposerVersionProcedure"],
    "post-install-cmd": [
      "Webship\\composer\\Procedures::createRequiredFilesProcedure",
      "@composer drupal-scaffold",
      "./bin/phing push"
    ],
    "post-update-cmd": [
      "Webship\\composer\\Procedures::createRequiredFilesProcedure",
      "./bin/phing push"
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-8.x-1.x": "8.1.x-dev"
    },
    "installer-paths": {
      "web/core": ["drupal/core"],
      "web/profiles/{$name}": ["type:drupal-profile"],
      "web/modules/contrib/{$name}": ["type:drupal-module"],
      "web/themes/contrib/{$name}": ["type:drupal-theme"],
      "web/libraries/{$name}": ["type:drupal-library"],
      "web/modules/custom/{$name}": ["type:drupal-custom-module"],
      "web/themes/custom/{$name}": ["type:drupal-custom-theme"],
      "drush/contrib/{$name}": ["type:drupal-drush"]
    },
    "enable-patching": true,
    "composer-exit-on-patch-failure": true,
    "patches": {
      "drupal/page_manager": {
        "Issue #2918564: Update 'page_manager.variant_route_filter' service according to core changes":
        "https://www.drupal.org/files/issues/2918564-22.patch"
      }
    }
  }
}
